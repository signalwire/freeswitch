  ap_last="${ap_cur}"
  ap_cur=`eval "echo ${ap_cur}"`
done
arg="${ap_cur}"

    echo "\"$arg\" \\" >> config.nice
  done
  echo '"$@"' >> config.nice
  chmod +x config.nice



