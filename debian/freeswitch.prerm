#!/bin/sh
set -e

case "$1" in
  remove|upgrade|deconfigure)
    if [ -d /etc/freeswitch ]; then
      echo "We're about to remove a configured FreeSWITCH..." >&2
    fi
    ;;
  failed-upgrade)
    ;;
  *)
    echo "prerm called with unknown argument \`$1'" >&2
    exit 1
    ;;
esac

#DEBHELPER#
exit 0
